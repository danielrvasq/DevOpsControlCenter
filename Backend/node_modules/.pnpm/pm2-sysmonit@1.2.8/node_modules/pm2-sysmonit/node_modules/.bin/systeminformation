#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/systeminformation@5.27.10/node_modules/systeminformation/lib/node_modules:/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/systeminformation@5.27.10/node_modules/systeminformation/node_modules:/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/systeminformation@5.27.10/node_modules:/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/systeminformation@5.27.10/node_modules/systeminformation/lib/node_modules:/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/systeminformation@5.27.10/node_modules/systeminformation/node_modules:/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/systeminformation@5.27.10/node_modules:/mnt/c/Users/drueda/Desktop/DevOps-Control_Centrel♦/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../systeminformation@5.27.10/node_modules/systeminformation/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../systeminformation@5.27.10/node_modules/systeminformation/lib/cli.js" "$@"
fi
